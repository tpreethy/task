<% provide(:title, 'Listing Schedules') %>
<h1>Listing Schedules</h1>
<table>
  <tr>
    <th>Pet Name</th>
    <th>Reason</th>
    <th>Date of next appointment</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
  <% @schedules.each do |schedule| %>
      <tr>
        <td><%= schedule.name %></td>
        <td><%=schedule.reason %></td>
        <td><%= schedule.dated %></td>
        <td><%= link_to 'Show', schedule %></td>
        <td><%= link_to 'Edit', edit_schedule_path(schedule) %></td>
        <td><%= link_to 'Destroy', schedule_path(schedule),
                        method: :delete, data: { confirm: 'Are you sure?' } %></td>

      </tr>
  <% end %>
</table>
 <%= link_to 'New Schedule', new_schedule_path %>